<div class="jumbotron">
  <div class="hero">
    <h1>URL Lockbox...</h1>
    <button type="read" class="btn btn-secondary btn-sm" id="filter-read-urls" name="button">See read URL's</button>
    <button type="read" class="btn btn-secondary btn-sm" id="filter-unread-urls" name="button">See unread URL's</button>
    <button type="read" class="btn btn-secondary btn-sm" id="sort-alphabetically" name="button">Sort Alphabetically</button>
    <br><br>
    <p>Search: </p><input type="text" id="searchUrls" placeholder="Search URL's">
    <br><br>
    <p>Enter a URL title and a valid URL.</p>
    <div class="form">
      <form>
        Title:<br>
        <input type="text" name="title" id="url-title"><br>
        URL:<br>
        <input type="text" name="url" id="url-url"><br><br>
      <input type="submit" value="Submit a link" id="create-url" class="btn btn-primary btn-md"><br><br>
      </form>
    </div>

    <ul id="list-of-urls">
    </ul>
  </div>
</div>
